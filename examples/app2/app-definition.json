{  
  "id":"dcos-maven-spring-sample",
  "cpus":1,
  "mem":512,
  "ports":[  
    0
  ],
  "instances":1,
  "container":{  
    "type":"DOCKER",
    "docker":{  
      "image":"unterstein/dcos-maven-spring-sample:latest",
      "network":"BRIDGE",
      "forcePullImage":true,
      "portMappings":[  
        {  
          "hostPort":0,
          "containerPort":8080,
          "protocol":"tcp"
        }
      ]
    }
  },
  "healthChecks":[  
    {  
      "protocol":"HTTP",
      "path":"/",
      "portIndex":0,
      "timeoutSeconds":10,
      "gracePeriodSeconds":10,
      "intervalSeconds":2,
      "maxConsecutiveFailures":10
    }
  ]
}
